{
  "name": "npm-run-scripts",
  "version": "1.2.0",
  "description": "run npm scripts",
  "keywords": ["npm", "yarn", "scripts", "run", "script"],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "scripts": {
    "update-dependencies": "yarn upgrade-interactive --latest",
    "release": "np --no-yarn --no-cleanup"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "npm-run": "cli.js",
    "yarn-run": "cli.js",
    "nr": "cli.js",
    "yr": "cli.js"
  },
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^3.2.4",
    "enquirer": "^2.3.0",
    "execa": "^1.0.0",
    "has-yarn": "^2.0.0",
    "read-pkg-up": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "files": ["LICENSE", "README.md", "cli.js", "package.json"],
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.15.1",
    "eslint-config-fisker": "^4.2.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "markdownlint-cli": "^0.14.0",
    "np": "^4.0.2",
    "prettier": "^1.16.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
